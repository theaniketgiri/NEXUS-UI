{
  "name": "naxusui",
  "version": "1.0.1",
  "description": "A CLI tool for scaffolding customizable React Native components",
  "private": false,
  "type": "module",
  "bin": {
    "nexus-ui": "./dist/cli.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "check-disk-space": "^3.4.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "enquirer": "^2.4.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "semver": "^7.5.4",
    "typescript": "^5.3.3",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/semver": "^7.5.6",
    "@types/validate-npm-package-name": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "react-native",
    "ui",
    "components",
    "cli",
    "nexus-ui"
  ],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
